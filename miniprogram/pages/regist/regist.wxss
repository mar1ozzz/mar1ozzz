page {
  padding-bottom: 90rpx;
  
}

.container {
  padding: 20rpx 0 30rpx 0;
 text-align: center;
  
}
.logo{
  margin-top: 30rpx;
  height: 300rpx;
  width: 300rpx ;
  margin-right: auto;
  border-radius: 5rpx;
  
}
tui-list-cell{ 
  margin-top: 20rpx;
  border-radius: 20rpx;
  border-bottom: 1px solid gray;
  display: flex;
 
}

.tui-line-cell { 
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;  
}

.tui-title {
  line-height: 32rpx;
  font-size: 40rpx;
  flex-shrink: 0;
}

.tui-input {
  font-size: 40rpx; 
  text-align: center;
  
}
.login-form{
  width:80%;
  display:flex;
  margin: 0 auto;
  background-color: transparent;
 
}
  

.set-right-text{
  padding-top: 13px;
  padding-bottom:13px;
}

.phcolor{
  font-size: 40rpx;
}

.img-body {
  width: 100%;
}

.img-header {
  display: flex;
  font-size: 26rpx;
  align-items: center;
  margin-bottom: 20rpx;
  justify-content: space-between;
  width: 100%;
}

.img-title {
  font-size: 28rpx;
  color: #000000;
}

.img-num {
  color: #999;
  font-size: 24rpx;
}

.footer-btn {
  height: 90rpx;
  line-height: 90rpx;
  width: 100vw;
  position: fixed;
  bottom: 50rpx;
  left: 0;
  right: 0;
  background-color: #f06c7a;
  text-align: center;
  color: #ffffff;
  font-size: 40rpx;
  font-weight: bold;
  border-radius: 20rpx ;
  margin: 0rpx;
}


.image-item {
  width: 210rpx !important;
  height: 210rpx !important;
}

.note{
  width: 80%;
  color:red;
  padding-left: 50rpx;
  margin-top:30rpx;

}